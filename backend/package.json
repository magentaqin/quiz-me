{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for quizme.",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "dev": "egg-bin dev",
    "start": "egg-scripts start --title=quizme-server-backend",
    "stop": "egg-scripts stop --title=quizme-server-backend",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts --resolve-plugins-relative-to . --fix",
    "clean": "ets clean"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "egg": "^2.6.1",
    "egg-scripts": "^2.6.0",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^4.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@eggjs/tsconfig": "^1.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^2.1.0",
    "egg-bin": "^4.11.0",
    "egg-mock": "^3.16.0",
    "eslint": "^8.0.0",
    "eslint-config-egg": "^12.0.0",
    "typescript": "^4.0.0",
    "@prisma/client": "^3.15.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "ci": {
    "version": "14, 16, 18"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "qinmu",
  "license": "MIT"
}
